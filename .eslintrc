{
  "env": {
      "browser": true
  },
  "extends": [
    "airbnb"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
    "react/jsx-props-no-spreading": [0],
    "react/static-property-placement": [0],
    "padded-blocks": [2, { "classes": "always" }]
  },
  "settings": {
    "import/core-modules": [
      "@babel/register",
      "@storybook/react",
      "@storybook/addon-actions",
      "mini-css-extract-plugin",
      "on-build-webpack",
      "replace-in-file",
      "shelljs"
    ]
  }
}
